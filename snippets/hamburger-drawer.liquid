{{ 'hamburger-menu.css' | asset_url | stylesheet_tag }}
<div class="hamburger-menu" id="hamburger-menu">
  <div class="hamburger-top flex">
    <a href="javascript:void(0);" class="hamburger-trigger close-toggle">{% render 'icon-close1' %}</a>
    <a href="{{ routes.root_url }}" class="hamburger-logo">
     <svg width="195" height="75" viewBox="0 0 195 75" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
      <rect width="195" height="75" fill="url(#pattern0)"/>
      <defs>
      <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
      <use xlink:href="#image0_378_378" transform="matrix(0.00215517 0 0 0.00560345 0 -0.00431034)"/>
      </pattern>
      <image id="image0_378_378" width="464" height="180" xlink:href="data:image/jpeg;base64,"/>
      </defs>
      </svg>
    </a>
  </div>  
  <ul class="top-menu menu--L-1">
    {% for l_1 in linklists.main-menu-2-new.links %}
    <li class="menu--item-1 {% if l_1.active %}L-1--active{% endif %} {% if l_1.child_active %}L-1--active{% endif %}">
      <a href="{{ l_1.url }}" class="menu--item-link {% if l_1.active %}active--link{% endif %}">{{ l_1.title }}</a>
      {% if l_1.links != blank %}<span class="l-1-trigger trigger-L2--active">{% render 'icon-menu' %}</span>{% endif %}

      {% comment %}L2{% endcomment %}
      <ul class="menu--L-2">
        {% for l_2 in l_1.links %}
          <li class="menu--item-2 {% if l_2.active %}L-2--active{% endif %} {% if l_2.child_active %}L-2--active{% endif %}">
            <a href="{{ l_2.url }}" class="menu--item-link {% if l_2.active %}active--link{% endif %}">{{ l_2.title }}</a>
            {% if l_2.links != blank %}<span class="l-2-trigger">{% render 'icon-menu' %}</span>{% endif %}
            
            {% comment %}L3{% endcomment %}
            <ul class="menu--L-3">
              {% for l_3 in l_2.links %}
              <li class="menu--item-3 {% if l_3.active %} L-3--active{% endif %} {% if l_3.child_active %}L-3--active{% endif %}">
                <a href="{{ l_3.url }}" class="menu--item-link {% if l_3.active %}active--link{% endif %}">{{ l_3.title | escape }}</a>
              </li>
              {% endfor %}
            </ul>
            
          </li>
        {% endfor %}
      </ul>   
      
    </li>
    <hr/>
    {% endfor %}
  </ul>
</div>
<script>
document.addEventListener("DOMContentLoaded", function () {
  const l1Triggers = document.querySelectorAll('.l-1-trigger');
  const l2Triggers = document.querySelectorAll('.l-2-trigger');
  const l3Triggers = document.querySelectorAll('.l-3-trigger');

  l1Triggers.forEach(trigger => {
    trigger.addEventListener('click', function() {
      const parentMenuItem = trigger.closest('.menu--item-1');
      parentMenuItem.classList.toggle('L-1--active');
      
      const otherL2Submenus = document.querySelectorAll('.menu--L-2--active');
      otherL2Submenus.forEach(submenu => {
        if (submenu !== parentMenuItem.querySelector('.menu--L-2')) {
          submenu.classList.remove('menu--L-2--active');
        }
      });

      const l2TriggersActive = document.querySelectorAll('.trigger-L2--active');
      l2TriggersActive.forEach(l2Trigger => {
        l2Trigger.classList.remove('trigger-L2--active');
      });
    });
  });
  
  l2Triggers.forEach(trigger => {
    trigger.addEventListener('click', function() {
      const parentMenuItem = trigger.closest('.menu--item-2');
      const submenuL3 = parentMenuItem.querySelector('.menu--L-3');
      submenuL3.classList.toggle('menu--L-3--active');
      trigger.classList.toggle('trigger-L2--active');
    });
  });

  l3Triggers.forEach(trigger => {
    trigger.addEventListener('click', function() {
      trigger.classList.toggle('trigger-L3--active');
    });
  });
});
</script>